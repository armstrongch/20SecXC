{
    for (var j = -128; j &lt; room_height; j += 64)
    {
        instance_create(i*64, j, Tree_obj);
        instance_create(room_width-64-i*64, j, Tree_obj);
    }
}

with (ScrollManager_obj)
{
    rightmost_trail_x = instance_nearest(room_width, room_height, Trail_obj).x;
    leftmost_trail_x = instance_nearest(0, room_height, Trail_obj).x;
