    player_distance_from_finish = max_player_distance_from_finish;
    scroll_speed = Player_obj.slow_run_speed;
    
    next_15_marker = max_player_distance_from_finish*0.9;
}

with (Trail_obj)
{
    
    image_alpha = 1 - (ScrollManager_obj.rightmost_trail_x-x)/64*0.1;
}

